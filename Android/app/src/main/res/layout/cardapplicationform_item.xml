<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="31dp"
        tools:layout_editor_absoluteY="0dp">

        <TextView
            android:id="@+id/tv_cardappformid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/application" />

        <TextView
            android:id="@+id/tv_firstname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/tv_cardappformid"
            android:layout_marginStart="30dp"
            android:text="First Name"
            android:textAlignment="center"
            android:textColor="@color/md_blue_900" />

        <TextView
            android:id="@+id/tv_lastname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/tv_firstname"
            android:layout_marginStart="30dp"
            android:text="Last Name"
            android:textAlignment="center"
            android:textColor="@color/md_blue_900" />

        <TextView
            android:id="@+id/tv_personalnumber"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tv_firstname"
            android:layout_below="@+id/tv_lastname"
            android:text="Personal number"
            android:textAlignment="center"
            android:textColor="@color/md_blue_900" />

            <TextView
                android:id="@+id/tv_dateofbirth"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_personalnumber"
                android:layout_marginStart="30dp"
                android:text="Date of birth"
                android:textAlignment="center"
                android:textColor="@color/md_blue_900" />

            <TextView
                android:id="@+id/tv_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_dateofbirth"
                android:layout_marginStart="30dp"
                android:text="Address"
                android:textAlignment="center"
                android:textColor="@color/md_blue_900" />


            <TextView
                android:id="@+id/tv_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_address"
                android:layout_toStartOf="@+id/iv_selfie"
                android:text="Phone number"
                android:textAlignment="center"
                android:textColor="@color/md_blue_900" />

            <TextView
                android:id="@+id/tv_email"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_address"
                android:layout_marginStart="30dp"
                android:text="E-mail"
                android:textAlignment="center"
                android:textColor="@color/md_blue_900" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/tv_email"
                android:text="Selfie" />

            <ImageView
                android:id="@+id/iv_selfie"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignParentEnd="true"
                android:layout_below="@+id/tv_email"
                android:layout_marginEnd="20dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tv_email"
                android:layout_toStartOf="@+id/iv_selfie"
                android:text="Driving license" />

            <ImageView
                android:id="@+id/iv_drivinglic"
                android:layout_width="100dp"
                android:layout_height="100dp"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/tv_email"
                android:layout_marginStart="48dp" />-->


        <TextView
            android:id="@+id/tv_dateofsubmission"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/tv_firstname"
            android:layout_below="@+id/tv_personalnumber"
            android:text="Date of submission"
            android:textAlignment="center"
            android:textColor="@color/md_blue_900" />

        <TextView
            android:id="@+id/tv_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/tv_dateofsubmission"
            android:layout_marginStart="30dp"
            android:text="Status"
            android:textAlignment="center"
            android:textColor="@color/md_blue_900" />

        <!--  <TextView
              android:id="@+id/tv_type"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_below="@+id/tv_dateofsubmission"
              android:layout_centerHorizontal="true"
              android:text="Type"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_paymentinfo"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentStart="true"
              android:layout_below="@+id/tv_type"
              android:layout_marginStart="30dp"
              android:text="Paymant information"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_drivinlic_number"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentStart="true"
              android:layout_below="@+id/tv_paymentinfo"
              android:layout_marginStart="30dp"
              android:text="Driving license number"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_drivinlic_country"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentStart="true"
              android:layout_below="@+id/tv_drivinlic_number"
              android:layout_marginStart="30dp"
              android:text="Driving license country"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_dateofevent"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentStart="true"
              android:layout_below="@+id/tv_drivinlic_country"
              android:layout_marginStart="30dp"
              android:text="Date of event"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_placeofevent"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignEnd="@+id/tv_oldcarddateofexpiry"
              android:layout_below="@+id/tv_drivinlic_country"
              android:text="Place of event"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignEnd="@+id/tv_dateofevent"
              android:layout_alignParentBottom="true"
              android:layout_marginBottom="195dp"
              android:text="Old card picture" />

          <ImageView
              android:id="@+id/iv_oldcardpic"
              android:layout_width="100dp"
              android:layout_height="100dp"
              android:layout_alignEnd="@+id/tv_dateofevent"
              android:layout_alignParentBottom="true"
              android:layout_marginBottom="81dp" />


          <TextView
              android:id="@+id/tv_oldcardcountry"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="138dp"
              android:text="Old card country"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />


          <TextView
              android:id="@+id/tv_oldcardauthority"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentBottom="true"
              android:layout_centerHorizontal="true"
              android:layout_marginBottom="101dp"
              android:text="Old card authority"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_oldcardnumber"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_above="@+id/iv_oldcardpic"
              android:layout_centerHorizontal="true"
              android:text="Old card number"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_oldcarddateofexpiry"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentBottom="true"
              android:layout_toEndOf="@+id/tv_personalnumber"
              android:text="Old card date of expiry"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_newaddress"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_above="@+id/tv_oldcarddateofexpiry"
              android:layout_toEndOf="@+id/tv_dateofbirth"
              android:text="New address"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_newFirsName"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_above="@+id/tv_oldcarddateofexpiry"
              android:layout_toEndOf="@+id/tv_newaddress"
              android:text="New first name"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_newLastName"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignParentEnd="true"
              android:layout_alignTop="@+id/tv_renewalreason"
              android:layout_marginEnd="75dp"
              android:text="New last name"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_above="@+id/tv_oldcarddateofexpiry"
              android:layout_alignEnd="@+id/tv_address"
              android:text="New selfie" />

          <ImageView
              android:id="@+id/iv_newselfie"
              android:layout_width="100dp"
              android:layout_height="100dp"
              android:layout_above="@+id/tv_oldcarddateofexpiry"
              android:layout_toEndOf="@+id/tv_cardappforms_title" />

          <TextView
              android:id="@+id/tv_renewalreason"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignEnd="@+id/tv_dateofbirth"
              android:layout_alignParentBottom="true"
              android:layout_marginBottom="48dp"
              android:text="Renewal reason"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_details"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_alignTop="@+id/tv_renewalreason"
              android:layout_toEndOf="@+id/tv_personalnumber"
              android:text="Details"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:id="@+id/tv_checkcode"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_above="@+id/tv_oldcarddateofexpiry"
              android:layout_alignEnd="@+id/iv_selfie"
              android:text="Check code"
              android:textAlignment="center"
              android:textColor="@color/md_blue_900" />

          <TextView
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_below="@id/tv_checkcode"
              android:text="Signature" />

          <ImageView
              android:id="@+id/iv_signature"
              android:layout_width="100dp"
              android:layout_height="100dp"
              android:layout_alignParentBottom="true"
              android:layout_marginBottom="74dp"
              android:layout_toEndOf="@+id/tv_oldcardauthority" /> />-->




    </RelativeLayout>

</android.support.constraint.ConstraintLayout>